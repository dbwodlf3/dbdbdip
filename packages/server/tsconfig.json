{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "resolveJsonModule": true,
        "target": "ES5",
        "module": "CommonJS",
        "inlineSourceMap": true,
        "removeComments": true,
        "strict": true,
        "outDir": "./dest",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "downlevelIteration": true
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": []
}